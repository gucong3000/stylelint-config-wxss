{
  "name": "stylelint-config-wxss",
  "version": "2.2.0",
  "description": "The WeChat wxss syntax config for stylelint",
  "keywords": [
    "stylelint",
    "stylelint-config",
    "standard"
  ],
  "author": "gucong3000",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gucong3000/stylelint-config-wxss.git"
  },
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "devDependencies": {
    "eslint": "^3.16.1",
    "eslint-config-stylelint": "^6.0.0",
    "jest": "^19.0.1",
    "npm-run-all": "^4.0.2",
    "npmpub": "^3.0.1",
    "remark-cli": "^3.0.0",
    "remark-preset-lint-consistent": "^2.0.0",
    "remark-preset-lint-recommended": "^1.0.0",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0"
  },
  "peerDependencies": {
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0"
  },
  "scripts": {
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint:md": "remark . --quiet --frail",
    "lint": "npm-run-all --parallel lint:*",
    "pretest": "npm run lint",
    "release": "npmpub",
    "test": "jest",
    "watch": "jest --watch"
  },
  "eslintConfig": {
    "extends": [
      "stylelint"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true
  },
  "remarkConfig": {
    "presets": [
      "lint-recommended",
      "lint-consistent"
    ]
  }
}
